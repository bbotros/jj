@model IEnumerable<JJTrailer.Library.JobCategory>

@{
    ViewBag.Title = "JobCategories";
    Layout = "~/Views/Shared/_careerlayout.cshtml";
}

<h2>JobCategories</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.JobCategoryName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.JobCategoryName)
        </td>
        
    </tr>
}

</table>
