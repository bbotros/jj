                    <li>@Html.ActionLink("Specifications", "Index", "specifications", new { area = "Admin" }, null)</li>
                    <li>@Html.ActionLink("Options", "Index", "Options", new { area = "Admin" }, null)</li>

                </ul>

            </div>
        </div>
    </div>
