
<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">

    <!--Dashboard-->
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="headingOne-1">
            <h4 class="panel-title">
                <a data-parent="#accordion" href=@Url.Action("Index", "Default", new { area="Admin"})>
                    Dashboard
                </a>

            </h4>
        </div>
    </div>
    <!--Catalog-->
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="headingOne">
            <h4 class="panel-title">
                <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                    Catalog
                </a>
            </h4>
        </div>
        <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
            <div class="panel-body">
                <ul class="nav nav-pills nav-stacked">
                    <li>@Html.ActionLink("Departments", "Index", "Departments", new { area = "Admin" }, null)</li>
                    <li>@Html.ActionLink("Categories", "Index", "Categories", new { area = "Admin" }, null)</li>
                    <li>@Html.ActionLink("Products", "Index", "Products", new { area = "Admin" }, null)</li>
                    <li>@Html.ActionLink("Images Manager", "Index", "ImageLibs", new { area = "Admin" }, null)</li>
                    <li>@Html.ActionLink("3D Manager", "Index", "STLfilemanagers", new { area = "Admin" }, null)</li>

                </ul>

            </div>
        </div>
    </div>
 
    <!--Trailers-->
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="headingOne2">
