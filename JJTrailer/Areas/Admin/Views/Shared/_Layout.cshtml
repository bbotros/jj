<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("uistyles", required: false)


    <link href="~/Content/themes/base/menu.css" rel="stylesheet" />
</head>
<body>
    <div class="nav navbar-inverse navbar-static-top">
        <div class="container-fluid">

            <ul class="nav navbar-nav">
                <li>
                    @Html.ActionLink("JJ Trailer Store Front", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })


                </li>
            </ul>
        </div>
    </div>

    <div class="container-fluid">

        <div class="row">
            <div class="col-md-2 nomargin">
                @Html.Partial("_Adminmenu")
            </div>
            <div class="col-md-10 ">
                @RenderBody()
            </div>
        </div>




        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    <script src="~/Scripts/Jquery/jquery-ui-1.11.3.js"></script>

    <!--angular-->
    <script src="~/Scripts/Angular/angular.js"></script>
    <script src="~/Scripts/Angular/angular-resource.js"></script>
    <script src="~/Scripts/Angular/angular-route.js"></script>
    <script src="~/Scripts/Angular/angular-touch.js"></script>
    <script src="~/Scripts/Angular/angular-animate.js"></script>

    <!--Angular app-->
    <script src="~/AngularApp/App.js"></script>
    <script src="~/AngularApp/Controller.js"></script>
    <script src="~/AngularApp/Services.js"></script>
    <script src="~/AngularApp/Filters.js"></script>
    <script src="~/AngularApp/Animation.js"></script>

    @RenderSection("scripts", required: false)

</body>
</html>